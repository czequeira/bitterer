{
  "name": "bitterer",
  "version": "0.1.0",
  "description": "Spring ioc in nodejs",
  "keywords": [
    "ioc",
    "spring"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/czequeira/bitterer.git"
  },
  "bugs": {
    "url": "https://github.com/czequeira/bitterer/issues"
  },
  "homepage": "https://github.com/czequeira/bitterer#readme",
  "license": "MIT",
  "author": "Rozendo",
  "type": "commonjs",
  "main": "dist/index.js",
  "peerDependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "prebuild": "npm run clean",
    "build": "tsc --build --force",
    "watch": "tsc --watch --preserveWatchOutput",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.4",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "glob": "^11.0.3",
    "path": "^0.12.7",
    "reflect-metadata": "^0.2.2",
    "yaml": "^2.8.0"
  }
}
